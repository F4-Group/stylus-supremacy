$base = (1 / 24) * 100

body
  min-width (100% / var)
  max-width 100% / var
  display !0
  visibility not true is false
  padding 1+2px
  list = 1 2 3
  color 1 in list
  item = list[-1]
  range = 1...5
  font 14px/1.5
  math = 1+2- 3*(4/5) **6
  color = color is defined ? unit(num,'px') : white
  color := white
  color ?= white
  border-color #fff is a 'rgba'
  background-color #f00 - rgba(100,0,0,0.5)
  name = 'blue'
  lookup('light-'+name)
  font-size (num)em
  width 'calc(100% - %s)' % 40px
  height ($base * 4)rem
  fonts = proxima-nova-regular, Proxima Nova, Hiragino Kaku Gothic Pro, Helvetica, 'Microsoft YaHei New'